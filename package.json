{
  "private": true,
  "name": "sizes",
  "description": "see the size of text and files in deflate, gzip and brotli formats",
  "license": "MIT",
  "dependencies": {
    "@remix-run/react": "^1.2.1",
    "@remix-run/vercel": "^1.2.1",
    "@supabase/supabase-js": "^1.31.0",
    "content-type": "^1.0.4",
    "dotenv": "^16.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-dropzone": "^12.0.4",
    "remix": "^1.2.1",
    "zod": "^3.12.0"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.2.1",
    "@remix-run/serve": "^1.2.1",
    "@types/content-type": "^1.1.5",
    "@types/react": "^17.0.24",
    "@types/react-dom": "^17.0.9",
    "autoprefixer": "^10.4.2",
    "concurrently": "^7.0.0",
    "cross-env": "^7.0.3",
    "daisyui": "^2.0.9",
    "postcss": "^8.4.6",
    "tailwindcss": "^3.0.23",
    "typescript": "^4.1.2"
  },
  "engines": {
    "node": ">=14"
  },
  "sideEffects": false,
  "scripts": {
    "build": "npm run build:css && remix build",
    "build:css": "tailwindcss -m -i ./styles/app.css -o app/app.css",
    "dev": "concurrently --names=\"css,rmx\" \"yarn dev:css\" \"remix dev\"",
    "dev:css": "tailwindcss -w -i ./styles/app.css -o app/app.css",
    "postinstall": "remix setup node"
  }
}
